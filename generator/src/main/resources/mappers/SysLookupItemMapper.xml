<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.much.it.mapper.SysLookupItemMapper">


    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.much.it.entity.SysLookupItem">
        <id column="ID" property="id" />
        <result column="ITEM_CODE" property="itemCode" />
        <result column="ITEM_NAME" property="itemName" />
        <result column="ITEM_DESC" property="itemDesc" />
        <result column="SORT_INDEX" property="sortIndex" />
        <result column="LOOKUP_ID" property="lookupId" />
        <result column="ATTR1" property="attr1" />
        <result column="ATTR2" property="attr2" />
        <result column="ATTR3" property="attr3" />
        <result column="CREATE_DATE" property="createDate" />
        <result column="CREATE_BY" property="createBy" />
        <result column="LASTUPDATE_DATE" property="lastupdateDate" />
        <result column="LASTUPDATE_BY" property="lastupdateBy" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, ITEM_CODE, ITEM_NAME, ITEM_DESC, SORT_INDEX, LOOKUP_ID, ATTR1, ATTR2, ATTR3, CREATE_DATE, CREATE_BY, LASTUPDATE_DATE, LASTUPDATE_BY
    </sql>

</mapper>
